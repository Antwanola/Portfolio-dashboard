<!doctype html>
<html class="no-js" lang="en">


        <!-- Start Breadcrump Area  -->
        <div class="breadcrumb-area rn-bg-color ptb--120 bg_image bg_image--1" data-black-overlay="6">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="breadcrumb-inner pt--100 pt_sm--40 pt_md--50">
                            <h2 class="title">Projects</h2>
                            <ul class="page-list">
                                <li><a href="index.html">Home</a></li>
                                <li class="current-page">Porjects</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Breadcrump Area  -->
        <main class="page-wrapper">
            <!-- Start blog Area  -->
            <div class="rn-blog-area rn-section-gap bg_color--1">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="section-title section-title--2 text-center">
                                <h2 class="title">Latest Projects</h2>
                                <p>These are the very few projects i have embarked on. 
                                    These includes design, printing and development works.</p>
                            </div>
                        </div>
                    </div>
                    <div class="row mt--20">
                        {{#each projects}}
                        <!-- Start Blog Area  -->
                        <div class="col-lg-4 col-md-6 col-sm-6 col-12">
                            <div class="blog blog-style--1">
                                <div class="thumbnail">
                                    <a href="/project-detail/{{_id}}">
                                        <img class="w-500 " src=" /uploads/{{file}}" alt="Blog Images" />
                                    </a>
                                </div>
                                <div class="content">
                                    <p class="blogtype">{{category}}</p>
                                    <h4 class="title"><a href="/project-detail/{{_id}}">{{description}}</a>
                                    </h4>
                                    <div class="blog-btn">
                                        <a class="rn-btn text-white" href="/project-detail/{{_id}}">Read More</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- End Blog Area  -->
                        {{/each}}

                        
                    </div>
                </div>
            </div>
            <!-- End Blog Area  -->
        </main>
       

</html>